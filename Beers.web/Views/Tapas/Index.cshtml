@model List<Beers.web.Models.Tapas.TapasViewModel>

<h2>Index</h2>
<h3>Tapas Page</h3>

<div>
    <table>
        <tr>
            <th>
                Name
            </th>
            <th>
                Precio
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.Label(item.Name.ToString())
                </td>
                <td>
                    @Html.Label(item.Price.ToString())
                </td>
            </tr>
        }
    </table>
</div>

